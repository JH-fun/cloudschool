<?xml version="1.0" encoding="UTF-8"?>
<project>
	<name>KeChengGuanLi</name>
	<cname>课程管理</cname>
	<annotation>课程管理系统</annotation>
	<dataBase id="73" name="postgres" driverClass="org.postgresql.Driver"
		jdbcUrl="jdbc:postgresql://localhost:5432/webdesigner" userName="postgres"
		passWord="postgres" maxPoolSize="10" />
	<pojos>
		<pojo id="58" name="TBook" cname="书本" package="cn.canetwork.project.pojo"
			extends="TObject">
			<annotation>书本实体类</annotation>
			<property id="60" name="name" cname="名称" type="String"
				length="0">
				<annotation>书本的名称</annotation>
			</property>
			<property id="67" name="student" cname="学生" type="TStudent"
				length="0">
				<annotation>书本所属的学生</annotation>
				<relation er-type="many_to_one" pojo="cn.canetwork.project.pojo.TStudent"
					pojo-property="books" cascade="none" />
			</property>
		</pojo>
		<pojo id="37" name="TStudent" cname="学生" package="cn.canetwork.project.pojo"
			extends="TObject">
			<annotation>学生实体类</annotation>
			<property id="41" name="name" cname="名称" type="String"
				length="0">
				<annotation>实体类的名称</annotation>
			</property>
			<property id="52" name="teachers" cname="教师们" type="java.util.Set"
				length="0">
				<annotation>教师的集合</annotation>
				<relation er-type="many_to_many_from" pojo="cn.canetwork.project.pojo.TTeacher"
					pojo-property="students" cascade="none" />
			</property>
			<property id="65" name="books" cname="书本" type="java.util.Set"
				length="0">
				<annotation>学生拥有的书本集合</annotation>
				<relation er-type="one_to_many" pojo="cn.canetwork.project.pojo.TBook"
					pojo-property="student" cascade="none" />
			</property>
			<property id="45" name="sex" cname="性别" type="String"
				length="0">
				<annotation>实体类的性别</annotation>
			</property>
		</pojo>
		<pojo id="39" name="TTeacher" cname="教师" package="cn.canetwork.project.pojo"
			extends="TObject">
			<annotation>教师实体类</annotation>
			<property id="47" name="sex" cname="性别" type="String"
				length="0">
				<annotation>实体类的性别</annotation>
			</property>
			<property id="50" name="students" cname="学生们" type="java.util.Set"
				length="0">
				<annotation>学生的集合</annotation>
				<relation er-type="many_to_many_to" pojo="cn.canetwork.project.pojo.TStudent"
					pojo-property="teachers" cascade="none" />
			</property>
			<property id="43" name="name" cname="名称" type="String"
				length="0">
				<annotation>实体类的名称</annotation>
			</property>
		</pojo>
	</pojos>
	<actions></actions>
	<actions></actions>
</project>